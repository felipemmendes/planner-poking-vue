{
  "name": "planner-poking-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:vue": "vite",
    "generate:css": "npx tailwindcss -o ./src/assets/styles.css",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:vue": "run-p type-check build-only",
    "preview": "npm run build:css && vite preview",
    "build-only": "npm run build:css && vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "random-word-slugs": "^0.1.6",
    "socket.io-client": "^4.5.3",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vue-tsc": "^1.0.8"
  }
}
